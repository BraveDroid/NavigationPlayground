<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/register_nav_graph"
    app:startDestination="@id/enterUserProfileFragment"
    tools:ignore="UnusedNavigation">

    <fragment
        android:id="@+id/enterUserProfileFragment"
        android:name="com.bravedroid.navigation.EnterUserProfileFragment"
        android:label="EnterUserProfileFragment"
        tools:layout="@layout/fragment_enter_user_profile">
        <action
            android:id="@+id/action_enterUserProfileFragment_to_chooseUserPasswordFragment"
            app:destination="@id/chooseUserPasswordFragment" />
    </fragment>
    <fragment
        android:id="@+id/chooseUserPasswordFragment"
        android:name="com.bravedroid.navigation.ChooseUserPasswordFragment"
        android:label="ChooseUserPasswordFragment"
        tools:layout="@layout/fragment_choose_user_password" />
</navigation>
